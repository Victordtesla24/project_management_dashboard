# System Configuration
architecture: "arm64-darwin"
memory_limit: "8GB"
gpu_utilization: "enabled"
thread_count:4

# AI Model Configuration
model_preferences:
  primary: "Anthropic Claude 3-5 Sonnet 20241022"
  fallback: "deepseek-coder"

# Token Management
token_optimization:
  max_context: 8000
  response_limit: 4000
  cache_strategy: "aggressive"
  budget_management: "strict"
  compression: true

# Cost Management
cost_management:
  strategy: "minimal"
  budget_control:
    daily_limit: 0.95
    cost_per_token: 0.0000010

# Code Generation Rules
code_generation:
  style: "minimal"
  documentation: "essential"
  testing: "basic"
  patterns:
    minimal_code: true
    performance_first: true

# Quality Standards
quality:
  complexity_threshold: "low"
  lint_level: "low"

# Security
security:
  input_validation: "basic"
  output_sanitization: true
  workspace_boundary: "cursor/project_management_dashboard"
