{
    "collection_interval": 60,
    "processes": [
        {
            "name": "dashboard",
            "pattern": "python.*dashboard",
            "metrics": ["cpu", "memory", "threads"]
        },
        {
            "name": "websocket",
            "pattern": "python.*websocket",
            "metrics": ["cpu", "memory", "connections"]
        },
        {
            "name": "monitor",
            "pattern": "python.*monitor",
            "metrics": ["cpu", "memory", "threads"]
        }
    ]
}
